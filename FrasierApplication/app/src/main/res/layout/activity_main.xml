<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:id="@+id/frasiergui"
    android:background="#fffffff7">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/vitals"
        android:src="@drawable/vital"
        android:background="#ff52bbff"
        android:layout_alignTop="@+id/excersise"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:paddingLeft="30dip"
        android:paddingTop="15dip"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/excersise"
        android:src="@drawable/excersise"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/vitals"
        android:layout_toEndOf="@+id/vitals"
        android:background="#fffff67c"
        android:paddingLeft="30dip"
        android:paddingTop="15dip"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/calendar"
        android:src="@drawable/calendar"
        android:background="#ffff777e"
        android:paddingTop="20dip"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/vitals"
        android:layout_toLeftOf="@+id/excersise"
        android:layout_toStartOf="@+id/excersise" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/walk"
        android:src="@drawable/walk"
        android:layout_alignLeft="@+id/excersise"
        android:layout_alignStart="@+id/excersise"
        android:background="#ffd5b6ff"
        android:paddingLeft="30dip"
        android:paddingTop="20dip"
        android:layout_below="@+id/vitals" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/emergency"
        android:src="@drawable/emergency2"
        android:layout_alignLeft="@+id/walk"
        android:layout_alignStart="@+id/walk"
        android:background="#fffffffb"
        android:paddingLeft="30dip"
        android:layout_below="@+id/walk"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <!--<Button-->
        <!--style="?android:attr/buttonStyleSmall"-->
        <!--android:layout_width="wrap_content"-->
