        <!--android:layout_above="@+id/bluetoothButton" />-->

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gamesButton"
        android:src="@drawable/games"
        android:background="#ff75ff7f"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/walk"
        android:layout_toStartOf="@+id/walk"
        android:layout_below="@+id/walk"
        android:layout_alignParentBottom="true" />

</RelativeLayout>
